@model EducationViewModel

<div class="row">
    <div class="col-md-4">
        <div class="profile-img">
            <img src="@Url.Action("RenderImage", new { image = Model.File })" alt="" />
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        @Html.LabelFor(m => m.EndDate)
    </div>
    <div class="col-md-6">
        @Html.TextBoxFor(m => m.EndDate, new { @class = "form-control" })
        @Html.ValidationMessageFor(m => m.EndDate, "", new { @class = "text-danger" })
    </div>
</div>
<div class="row">
    @Html.ActionLink("Изменить", "EditEducation", "Education", new { educationId = Model.Id }, new { @class = "btn btn-group" })
    @Html.ActionLink("Удалить", "DeleteEducation", "Education", new { educationId = Model.Id }, new { @class = "btn btn-danger" })
</div>
<hr />

